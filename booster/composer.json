{
  "name": "jelix/booster-jelix-org",
  "type": "project",
  "description": "",
  "license": "LGPL-2.0-only",
  "authors": [
  ],
  "require": {
    "php": ">=7.4",
    "jelix/composer-module-setup": "^1.1.0",
    "jelix/jcommunity-module": "~1.3.17",
    "jelix/jtags-module": "1.1.*",
    "jelix/jelix": "dev-jelix-1.8.x",
    "jelix/wikirenderer": "3.1.*",
    "jelix/wikirenderer-plugin": "1.7.*",
    "jelix/feeds-module" : "1.7.*"
  },
  "minimum-stability": "stable",
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "allow-plugins": {
      "jelix/composer-module-setup": true
    }
  },
  "extra": {
    "jelix": {
      "target-jelix-version": "1.7",
      "app-dir" : "./",
      "var-config-dir" : "var/config/",
      "modules-dir" : [
        "modules/",
        "admin-modules/"
      ],
      "plugins-dir" : [
        "plugins/"
      ]
    }
  }
}
